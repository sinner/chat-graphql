{
   "name": "messenger-chat",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "type": "commonjs",
   "scripts": {
      "start": "ts-node --transpile-only --respawn src/index.ts",
      "dev": "nodemon src/index.ts",
      "typeorm": "typeorm-ts-node-commonjs",
      "migration:run": "typeorm migration:run -d ./src/data-source.ts"
   },
   "dependencies": {
      "cors": "^2.8.5",
      "dotenv": "^16.4.1",
      "express": "^4.18.2",
      "graphql": "^16.8.1",
      "graphql-yoga": "^5.1.1",
      "pg": "^8.4.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.19"
   },
   "devDependencies": {
      "@types/cors": "^2.8.17",
      "@types/express": "^4.17.21",
      "@types/node": "^16.11.10",
      "nodemon": "^3.0.3",
      "ts-node": "10.9.1",
      "typescript": "4.5.2"
   }
}
